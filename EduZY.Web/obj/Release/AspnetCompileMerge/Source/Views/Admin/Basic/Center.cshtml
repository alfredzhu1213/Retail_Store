                        <script type="text/javascript">
                            /*查看文件*/
                            function readDocInfo(pkId, resourceId, title, orgType) {
                                var url = "";
                                if (orgType && 'docIssued' == orgType) {
                                    url = "/docIssued/gatherShow1?docIssuedId=" + resourceId + "&isRead=true";
                                }
                                else if (orgType && 'docReport' == orgType) {
                                    url = "/docReport/toReportInfo?docReportId=" + resourceId;
                                }

                                top.$.ligerDialog.open({
                                    className: "dialog_style02",
                                    height: 400,
                                    width: 730,
                                    title: '查看文件',
                                    url: "" + url,
                                    dialogWaiting: true,
                                    buttons: [
                                {
                                    text: '确认签收',
                                    onclick: confirmSign
                                },
                                {
                                    text: '关闭',
                                    onclick: close
                                }]
                                });

                                //文件下发
                                function issued(button, dialogDom) {
                                    var url = "/docIssued/toIssuedAdd.do?selectDocIdStr=" + docIssuedId + "&doWhat=add&moduleId=";
                                    if (!easyloader.isIframe) {
                                        return;
                                    }
                                    dialogDom.close();

                                    var tabid = "";
                                    var tabOptions = {
                                        text: "" ? "" : '空菜单',
                                        url: url,
                                        tabid: tabid,
                                        resizeFrame: true,
                                        dialogWaiting: true,
                                        menuPic: ""
                                    };

                                    //判断是否存在，存在则覆盖，否则新增
                                    if (parent.navtab.isTabItemExist(tabid)) {
                                        //覆盖tab前事件
                                        parent.$.ligerDialog.confirm("你确定要覆盖" + "" + "项吗?", function (status) {
                                            if (status) {
                                                parent.navtab.overrideTabItem(tabid, tabOptions);
                                            }
                                        });
                                    } else {
                                        parent.navtab.addTabItem(tabOptions);
                                    }
                                }

                                function close(button, dialogDom) {
                                    dialogDom.close();
                                }

                                function confirmSign(button, dg) {
                                    var reqUrl = '';
                                    if (orgType && 'docIssued' == orgType) {
                                        reqUrl = '/docIssued/signDoc?docIssuedId=' + resourceId;
                                    }
                                    else if (orgType && 'docReport' == orgType) {
                                        reqUrl = '/docReport/signDoc?docReportId=' + resourceId;
                                    }
                                    $.ajax({
                                        type: 'POST',
                                        url: reqUrl,
                                        success: function (o) {
                                            var ret = jQuery.parseJSON(o);
                                            if (ret && ret.status == '1') {
                                                $.ligerDialog.success('确认签收成功', '提示', function () {
                                                    dg.close();
                                                    window.location.reload();
                                                });
                                            }
                                        },
                                        error: function () {
                                            $.ligerDialog.error('确认签收失败', '提示', function () {
                                                dg.close();
                                            });
                                        }
                                    });
                                }

                            }

                        </script>
