        function downloadFileReleasePub(content, title) {
            jQuery.ajax({
                type: "POST",
                url: contextPath + "/workFlow/hasFileReleasePub.do?content=" + content + "&moduleId=",
                success: function (o) {
                    var _e = jQuery.parseJSON(o);
