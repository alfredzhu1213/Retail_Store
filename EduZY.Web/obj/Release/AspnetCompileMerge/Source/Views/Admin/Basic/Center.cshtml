            using(["tree", "dialog"], function () {
                $(".tab", $(".ui_box_style03")).live("click", function () {
                    if (!easyloader.isIframe) {
                        return;
                    }

                    var hrefElement = $(this);
                    var tabid = hrefElement.attr("tabid");
                    var tabOptions = {
                        text: hrefElement.attr("tabtitle") ? hrefElement.attr("tabtitle") : '空菜单',
                        url: hrefElement.attr("url") ? hrefElement.attr("url") : '#',
                        tabid: tabid,
                        resizeFrame: true,
                        dialogWaiting: true,
                        menuPic: hrefElement.attr("menuPic")
                    };

                    //判断是否存在，存在则覆盖，否则新增
                    if (parent.navtab.isTabItemExist(tabid)) {
                        //覆盖tab前事件
                        parent.$.ligerDialog.confirm("你确定要覆盖" + hrefElement.attr("tabtitle") + "项吗?", function (status) {
                            if (status) {
                                parent.navtab.overrideTabItem(tabid, tabOptions);
                            }
                        });
                    } else {
                        parent.navtab.addTabItem(tabOptions);
                    }
                });
            });

        });

